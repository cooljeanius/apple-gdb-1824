	mkdir -p $(DSTDIR)
	dsymutil $(SYMROOT)/$(LIB)
	strip -S -o $(DSTDIR)/$(LIB) $(SYMROOT)/$(LIB)

clean-local:
	rm -f $(OBJS) $(SYMROOT)/$(LIB)

