	mkdir -p $(OBJROOT)
	$(CC) -c $(CFLAGS) $(AM_CFLAGS) $< -o $@

$(SYMROOT)/$(LIB) : $(OBJS)
	$(CC) $(CFLAGS) $(AM_CFLAGS) -o $@ -dynamiclib \
		-install_name $(GDBDIR)/$(LIB) \
		-single_module -compatibility_version 1 \
		-current_version 1 $(OBJS) -framework ApplicationServices

