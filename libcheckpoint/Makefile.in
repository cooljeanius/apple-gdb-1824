site.exp: Makefile $(EXTRA_DEJAGNU_SITE_CONFIG)
	$(AM_V_GEN)set -e; ( \
	echo '## these variables are automatically generated by make ##'; \
	echo '# Do not edit here.  If you wish to override these values'; \
	echo '# edit the last section'; \
