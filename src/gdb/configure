FCNTL_H_CFLAGS=""
       for ac_header in fcntl.h
do :
  ac_fn_c_check_header_compile "$LINENO" "fcntl.h" "ac_cv_header_fcntl_h" "$ac_includes_default"
if test "x$ac_cv_header_fcntl_h" = xyes
then :
  printf "%s\n" "#define HAVE_FCNTL_H 1" >>confdefs.h

  : # (OK, no special flags needed)

else case e in #(
  e)
         for ac_header in sys/fcntl.h
do :
  ac_fn_c_check_header_compile "$LINENO" "sys/fcntl.h" "ac_cv_header_sys_fcntl_h" "$ac_includes_default"
if test "x$ac_cv_header_sys_fcntl_h" = xyes
then :
  printf "%s\n" "#define HAVE_SYS_FCNTL_H 1" >>confdefs.h

    FCNTL_H_CFLAGS="${acl_cv_wp}-I/usr/include/sys"

else case e in #(
  e)
    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: fcntl.h is in neither of the places where we might expect it to be" >&5
printf "%s\n" "$as_me: WARNING: fcntl.h is in neither of the places where we might expect it to be" >&2;}
   ;;
esac
fi

done ;;
esac
fi

done
