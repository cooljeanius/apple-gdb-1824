  *-apple-darwin* | *-apple-macos*)
    stdflag=""
    if test "x${enable_build_with_cxx}" = "xyes"; then
      stdflag="-std=gnu++03"
    else
      stdflag="-std=gnu99"
    fi
    # -Wmost is APPLE_LOCAL, as are -Wextra-tokens and -Wnewline-eof
