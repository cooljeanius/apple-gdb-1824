	@dir=rdi-share; \
	if [ -f ./$${dir}/Makefile ]; then \
	  r=`pwd`; export r; \
	  srcroot=`cd $(srcdir); pwd`; export srcroot; \
	  (cd $${dir}; $(MAKE) $(FLAGS_TO_PASS) all); \
	else \
	  echo "no Makefile for '$${dir}'"; \
	fi

