		$(TDEPLIBS) $(TUI_LIBRARY) $(CLIBS) $(LOADLIBES)
	@echo "stripping __objcInit from $@";
	echo "__objcInit" > $(TMPDIR)/macosx-syms-to-remove;
	-if [ -f .libs/gdb$(EXEEXT) ]; then \
	  $(STRIP) -R $(TMPDIR)/macosx-syms-to-remove -X .libs/gdb$(EXEEXT); \
	else \
