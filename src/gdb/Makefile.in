	-(test -w $(TMPDIR) && rm -f $(AM_V_v) $(TMPDIR)/cfg_stat.sh)
	-rm -f config.status || \
	  (test -w $(TMPDIR) && mv -f $(AM_V_v) config.status $(TMPDIR)/cfg_stat.sh)

