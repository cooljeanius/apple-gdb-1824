	  echo "running autoconf" && autoconf; \
	elif test -e configure.sh; then \
	  echo "using backup $@ script"; \
	  cat configure.sh >$@; \
	  chmod a+x $@; \
	else \
	  echo "unsure what to do, so just touching $@" && touch $@; \
	fi

