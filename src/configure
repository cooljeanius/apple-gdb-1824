# Handle --enable-gold, --enable-ld.
# --disable-gold [--enable-ld]
#     Build only ld.  Default option.
# --enable-gold [--enable-ld]
#     Build both gold and ld.  Install gold as "ld.gold", install ld
#     as "ld.bfd" and "ld".
# --enable-gold=default [--enable-ld]
#     Build both gold and ld.  Install gold as "ld.gold" and "ld",
#     install ld as "ld.bfd".
# --enable-gold[=default] --disable-ld
#     Build only gold, which is then installed as both "ld.gold" and "ld".
# --enable-gold --enable-ld=default
#     Build both gold (installed as "ld.gold") and ld (installed as "ld"
#     and ld.bfd).
#     In other words, ld is default
# --enable-gold=default --enable-ld=default
#     Error(?) (make warning instead here?).

default_ld=""
# Check whether --enable-gold was given.
if test ${enable_gold+y}
then :
  enableval=$enable_gold; ENABLE_GOLD=${enableval}
else case e in #(
  e) ENABLE_GOLD=no ;;
esac
fi

case "${ENABLE_GOLD}" in
  yes|default)
    # Check for ELF target.
    is_elf=no
    case "${target}" in
      *-*-android | *-*-elf* | *-*-sysv4* | *-*-unixware* | *-*-eabi* \
      | hppa*64*-*-hpux* | *-*-linux* | *-*-gnu* | frv-*-uclinux* \
      | *-*-irix5* | *-*-irix6* | *-*-netbsd* | *-*-openbsd* \
      | *-*-freebsd* | *-*-dragonfly* | *-*-solaris2* | *-*-nto* \
      | *-*-nacl*)
        case "${target}" in
          *-*-linux*aout* | *-*-linux*oldld*)
            ;;
          *)
            is_elf=yes
            ;;
        esac
    esac

    if test "x${is_elf}" = "xyes"; then
      # Check for target supported by gold.
      case "${target}" in
        i?86-*-* | x86_64-*-* | sparc*-*-* | powerpc*-*-* | arm*-*-* \
        | aarch64*-*-* | tilegx*-*-*)
	  configdirs="${configdirs} gold"
	  if test "x${ENABLE_GOLD}" = "xdefault"; then
	    default_ld=gold
	  fi
	  ENABLE_GOLD=yes
          ;;
      esac
    fi
    ;;
  no)
    ;;
  *)
    as_fn_error $? "invalid --enable-gold argument" "$LINENO" 5
    ;;
esac

# Check whether --enable-ld was given.
if test ${enable_ld+y}
then :
  enableval=$enable_ld; ENABLE_LD=${enableval}
else case e in #(
  e) ENABLE_LD=yes ;;
esac
fi

case "${ENABLE_LD}" in
  default)
    if test "x${default_ld}" != "x"; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: either gold or ld can be the default ld" >&5
printf "%s\n" "$as_me: WARNING: either gold or ld can be the default ld" >&2;}
    fi
    ;;
  yes)
    ;;
  no)
    if test "x${ENABLE_GOLD}" != "xyes"; then
      { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: neither ld nor gold are enabled" >&5
printf "%s\n" "$as_me: WARNING: neither ld nor gold are enabled" >&2;}
    fi
    configdirs=`echo " ${configdirs} " | sed -e 's/ ld / /'`
    ;;
  *)
    as_fn_error $? "invalid --enable-ld argument" "$LINENO" 5
    ;;
esac

