# Allow host libstdc++ to be specified for static linking with PPL.

# Check whether --with-host-libstdcxx was given.
if test ${with_host_libstdcxx+y}
then :
  withval=$with_host_libstdcxx;
fi

case ${with_host_libstdcxx} in
  no|yes)
    as_fn_error $? "the --with-host-libstdcxx flag needs an argument to it" "$LINENO" 5
    ;;
esac

# Libraries to use for stage1 or when not bootstrapping.

# Check whether --with-stage1-libs was given.
if test ${with_stage1_libs+y}
then :
  withval=$with_stage1_libs; if test "x${withval}" = "xno" -o "x${withval}" = "xyes"; then
   stage1_libs=""
 else
   stage1_libs=${withval}
 fi
else case e in #(
  e) stage1_libs=${with_host_libstdcxx} ;;
esac
fi


# Linker flags to use for stage1 or when not bootstrapping.

# Check whether --with-stage1-ldflags was given.
if test ${with_stage1_ldflags+y}
then :
  withval=$with_stage1_ldflags; if test "x${withval}" = "xno" -o "x${withval}" = "xyes"; then
   stage1_ldflags=""
 else
   stage1_ldflags=${withval}
 fi
else case e in #(
  e) stage1_ldflags=""
 # In stage 1, default to linking libstdc++ and libgcc statically with GCC
 # if supported. But if the user explicitly specified the libraries to use,
 # trust that they are doing what they want.
 if test "x${stage1_libs}" = "x" -a "x${have_static_libs}" = "xyes"; then
   stage1_ldflags="-static-libstdc++ -static-libgcc"
 fi ;;
esac
fi


# Libraries to use for stage2 and later builds.  This defaults to the
# argument passed to --with-host-libstdcxx.

# Check whether --with-boot-libs was given.
if test ${with_boot_libs+y}
then :
  withval=$with_boot_libs; if test "x${withval}" = "xno" -o "x${withval}" = "xyes"; then
   poststage1_libs=""
 else
   poststage1_libs=${withval}
 fi
else case e in #(
  e) poststage1_libs=${with_host_libstdcxx} ;;
esac
fi


# Linker flags to use for stage2 and later builds.

# Check whether --with-boot-ldflags was given.
if test ${with_boot_ldflags+y}
then :
  withval=$with_boot_ldflags; if test "x${withval}" = "xno" -o "x${withval}" = "xyes"; then
   poststage1_ldflags=""
 else
   poststage1_ldflags=${withval}
 fi
else case e in #(
  e) poststage1_ldflags=""
 # In stages 2 and 3, default to linking libstdc++ and libgcc
 # statically. But if the user explicitly specified the libraries to
 # use, trust that they are doing what they want.
 if test "x${poststage1_libs}" = "x"; then
   poststage1_ldflags="-static-libstdc++ -static-libgcc"
 fi ;;
esac
fi


