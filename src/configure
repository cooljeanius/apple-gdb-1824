    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: checking for version . of ISL" >&5
printf %s "checking for version . of ISL... " >&6; }
    if test "$cross_compiling" = yes
then :
  gcc_cv_isl=yes
else case e in #(
  e) cat confdefs.h - <<_ACEOF >conftest.$ac_ext
