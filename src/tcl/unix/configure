    { printf "%s\n" "$as_me:${as_lineno-$LINENO}: WARNING: \"64bit support being disabled -- do NOT know magic for this platform\"" >&5
printf "%s\n" "$as_me: WARNING: \"64bit support being disabled -- do NOT know magic for this platform\"" >&2;}
