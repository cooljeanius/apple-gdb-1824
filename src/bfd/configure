    "broken_libtool_hack_fix3":C)
if test -x ./config.status; then
  if test -z "${our_libtool_script}"; then
    if test -x ./libtool; then
      our_libtool_script="./libtool"
    elif test -n "${srcdir}" -a -x ${srcdir}/libtool; then
